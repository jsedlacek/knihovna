{
  "name": "knihovna",
  "version": "1.0.0",
  "description": "Czech e-books static site",
  "type": "module",
  "imports": {
    "#@/*": "./src/*"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler deploy",
    "cf-typegen": "wrangler types",
    "type-check": "tsc --noEmit",
    "check": "npm run type-check && npm run lint && npm run format:check && npm test",
    "scrape": "node src/scripts/scrape-books.ts",
    "test": "node --test src/**/*.test.ts",
    "test:watch": "node --test --watch src/**/*.test.ts",
    "lint": "oxlint src/",
    "lint:fix": "oxlint src/ --fix",
    "format": "biome format --write",
    "format:check": "biome format",
    "fix": "npm run lint:fix && npm run format",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@tanstack/react-router": "^1.136.5",
    "@tanstack/react-start": "^1.136.5",
    "@tanstack/start-static-server-functions": "^1.136.5",
    "@types/lodash-es": "^4.17.12",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.553.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.5",
    "@cloudflare/vite-plugin": "^1.14.2",
    "@storybook/react-vite": "^10.0.7",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/yargs": "^17.0.35",
    "@vitejs/plugin-react": "^5.1.1",
    "cheerio": "^1.1.2",
    "domhandler": "^5.0.3",
    "oxlint": "^1.28.0",
    "storybook": "^10.0.7",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "wrangler": "4.47.0",
    "yargs": "^18.0.0"
  }
}
