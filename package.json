  },
  "devDependencies": {
    "@astrojs/ts-plugin": "^1.10.4",
    "@tailwindcss/vite": "^4.1.12",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/yargs": "^17.0.32",
    "cheerio": "^1.1.2",
    "domhandler": "^5.0.3",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
