{
  "name": "knihovna",
  "version": "1.0.0",
  "description": "Czech e-books static site",
  "type": "module",
  "imports": {
    "#@/*": "./src/*"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
