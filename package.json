{
  "name": "knihovna",
  "version": "1.0.0",
  "description": "Czech e-books static site",
  "keywords": [],
  "license": "ISC",
  "author": "",
  "type": "module",
  "imports": {
    "#@/*": "./src/*"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler deploy",
    "cf-typegen": "wrangler types",
    "type-check": "tsc --noEmit",
    "check": "npm run type-check && npm run lint && npm run format:check && npm test",
    "scrape": "node src/scripts/scrape-books.ts",
    "test": "node --test src/**/*.test.ts",
    "test:watch": "node --test --watch src/**/*.test.ts",
    "lint": "oxlint src/",
    "lint:fix": "oxlint src/ --fix",
    "format": "oxfmt --write",
    "format:check": "oxfmt --check",
    "fix": "npm run lint:fix && npm run format",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.144.0",
    "@tanstack/react-start": "^1.145.0",
    "@tanstack/start-static-server-functions": "^1.145.0",
    "@types/lodash-es": "^4.17.12",
    "lodash-es": "^4.17.22",
    "lucide-react": "^0.562.0",
    "oxfmt": "^0.21.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.17.1",
    "@storybook/react-vite": "^10.1.10",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/yargs": "^17.0.35",
    "@vitejs/plugin-react": "^5.1.2",
    "cheerio": "^1.1.2",
    "domhandler": "^5.0.3",
    "oxlint": "^1.36.0",
    "storybook": "^10.1.10",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^8.0.0-beta.5",
    "wrangler": "4.54.0",
    "yargs": "^18.0.0"
  },
  "engines": {
    "node": "24",
    "pnpm": "10"
  },
  "volta": {
    "node": "24.11.1"
  },
  "pnpm": {
    "overrides": {
      "vite": "8.0.0-beta.5"
    }
  }
}
